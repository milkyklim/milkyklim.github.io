group :jekyll_plugins do
	gem "jekyll-paginate"
	gem "jekyll-sitemap"
	gem "jekyll-gist"
	gem "jekyll-feed"
	gem "jekyll-typogrify"
	gem "jekyll-tagging", :git => 'https://github.com/pattex/jekyll-tagging.git'
	gem "jemoji"
end
